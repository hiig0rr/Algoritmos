#include <stdio.h>
int main() {
    int num1, num2, num3, num4;
    int soma1, soma2;
    float resultado;
    printf("Digite os dois primeiros números inteiros:\n");
    scanf("%d %d", &num1, &num2);
    soma1 = num1 + num2;
    printf("Digite mais dois números inteiros:\n");
    scanf("%d %d", &num3, &num4);
    soma2 = num3 + num4;
    if (soma2 == 0) {
        printf("Não é possível dividir por zero.\n");
    } else {
        resultado = (float)soma1 / soma2;
        printf("O resultado da divisão da primeira soma pela segunda é: %.2f\n", resultado);
    }

    return 0;
}